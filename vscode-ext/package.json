{
  "name": "lllmcc",
  "displayName": "lllmcc",
  "description": "Completa c√≥digo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lllmcc.run",
        "title": "LLLMCC run"
      },
      {
        "command": "lllmcc.debug",
        "title": "LLLMCC debug selected"
      },
      {
        "command": "lllmcc.setURL",
        "title": "LLLMCC change service URL"
      }
    ],
    "keybindings": [
      {
        "command": "lllmcc.run",
